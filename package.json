{
  "name": "voicedrive-v100",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx src/server.ts",
    "build": "prisma generate && vite build",
    "build:vercel": "vite build",
    "build:strict": "tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview",
    "preview:pwa": "vite preview --port 4173",
    "type-check": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:integration": "jest --testMatch='**/integration*.test.ts'",
    "test:integration:server": "tsx src/api/server.integration-test.ts",
    "test:scenario1": "jest --testMatch='**/scenario1*.test.ts'",
    "test:scenario2": "jest --testMatch='**/scenario2*.test.ts'",
    "test:scenario4": "jest --testMatch='**/scenario4*.test.ts'",
    "test:scenario6": "jest --testMatch='**/scenario6*.test.ts'",
    "test:all-scenarios": "jest --testMatch='**/scenario*.test.ts'",
    "test:watch": "jest --watch",
    "secrets:deliver": "tsx src/cli/secret-cli.ts deliver",
    "secrets:retrieve": "tsx src/cli/secret-cli.ts retrieve",
    "secrets:status": "tsx src/cli/secret-cli.ts status",
    "watch:mcp": "node scripts/watch-mcp-shared.js",
    "mcp:sync": "node mcp-integration-server/scripts/check-mcp-sync.js",
    "db:seed": "npx tsx prisma/seed.ts",
    "seed:integration-test": "npx tsx scripts/seed-integration-test-data.ts",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:jwt": "tsx scripts/generate-test-jwt.ts",
    "test:posts-api": "tsx scripts/test-posts-api.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@tanstack/react-query": "^5.90.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.513.0",
    "node-cron": "^4.2.1",
    "prisma": "^6.16.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "react-swipeable": "^7.0.2",
    "react-toastify": "^11.0.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "inquirer": "^12.9.6",
    "jest": "^30.1.3",
    "mysql2": "^3.15.0",
    "ora": "^9.0.0",
    "postcss": "^8.4.32",
    "sharp": "^0.34.4",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.0",
    "terser": "^5.41.0",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-window": "^7.3.0"
  }
}
